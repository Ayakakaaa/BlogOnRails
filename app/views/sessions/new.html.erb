<h1>Log In</h1>
<% flash[:alert]%>
<%= form_with url: sessions_path, local: true do |f|%>
    <div>
        <%= f.label :email%>
        <%= f.email_field:email%>
    </div>

    <div>
        <%=f.label :password %>
        <%=f.password_field :password%>
    </div>

    <%=f.submit "Log In"%>
<%end%>
